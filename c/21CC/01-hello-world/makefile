CFLAGS = -Wall

help:
	@echo "Please use \`make <target>' where <target> is one of"
	@echo "  hello  to compile a hello world executable"
	@echo "  docs   to compile documentation"
	@echo "  clean  to remove files generated by 'docs' and 'hello'"
	@echo "  help   to print this message"

hello:

docs:
	doxygen Doxyfile

clean:
	rm -rf hello html latex

.PHONY: docs clean help
